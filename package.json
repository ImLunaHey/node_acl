{
  "name": "acl",
  "version": "1.0.0",
  "description": "An Access Control List module, based on Redis with Express middleware support",
  "keywords": [
    "middleware",
    "acl",
    "web"
  ],
  "repository": "git://github.com/flash-oss/node_acl.git",
  "author": "Manuel Astudillo <manuel@optimalbits.com>",
  "homepage": "https://github.com/flash-oss/node_acl",
  "engines": {
    "node": ">= 10"
  },
  "main": "./index.js",
  "dependencies": {
    "async": "^2.1.4",
    "bluebird": "^3.0.2",
    "lodash": "^4.17.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.0"
  },
  "optionalDependencies": {
    "mongodb": "^3.0.0",
    "redis": "^2.2.5"
  },
  "scripts": {
    "test": "npm run test_memory && npm run test_redis && npm run test_mongo && npm run test_mongo_single",
    "test_memory": "ACL_BACKEND=memory mocha",
    "test_redis": "ACL_BACKEND=redis mocha",
    "test_mongo": "ACL_BACKEND=mongo mocha",
    "test_mongo_single": "ACL_BACKEND=mongo_single mocha",
    "cover": "nyc npm run test"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  }
}
